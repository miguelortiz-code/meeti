<div class="contenedor proximos-eventos">
    <h1><%= namePage %></h1>
    <% if(meeties.length > 0){%>
    <div class="grid columnas-md-3 ">
        <% meeties.forEach(meeti =>{%>
            <%- include('../partials/cards', { meeti }) %>
        <%})%>
    </div>
    <%} else{%>
        <p>No hay meeti's en este categoria </p>
        <a href="/" class="btn btn-rosa">Volver al inicio</a>
    <%}%>
</div>