{
  "name": "meeti",
  "version": "1.0.0",
  "description": "Proyecto Meeti con NodeJs, PostgreSQL y EJS",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js --watch",
    "db:import": "node src/seed/seeder.js -i",
    "db:delete": "node src/seed/seeder.js -d",
    "js": "webpack --watch",
    "server": "concurrently \"npm run dev\" \"npm run js\" "
